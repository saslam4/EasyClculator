<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.AuthenticStudio.CalculatorSimplified.MainActivity">


    <EditText
        android:id="@+id/eTInput"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/btnNeg"/>

    <Button
        android:id="@+id/btnClear"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_above="@+id/btn9"
        android:layout_alignParentLeft="true"
        android:padding="10dp"
        android:text="@string/str_C"
        android:textSize="30sp" />
    <!--
        <EditText
            android:id="@+id/eTResult"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@+id/eTInput"
            android:layout_centerHorizontal="true" />
    -->
    <Button
        android:id="@+id/btnNeg"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btn1"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_toLeftOf="@+id/btn0"
        android:text="@string/str_sign"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btnDiv"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btnMult"
        android:layout_toLeftOf="@+id/btn7"
        android:text="@string/str_div"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btnMult"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/eTInput"
        android:layout_toLeftOf="@+id/btn7"
        android:text="@string/str_x"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/eTInput"
        android:layout_toLeftOf="@+id/btn9"
        android:padding="10dp"
        android:text="@string/str_8"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/eTInput"
        android:layout_toLeftOf="@+id/btn8"
        android:text="@string/str_7"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/eTInput"
        android:text="@string/str_9"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btnMinus"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btnDiv"
        android:layout_toLeftOf="@+id/btn4"
        android:text="@string/str_minus"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_toLeftOf="@+id/btn5"
        android:layout_below="@+id/btn7"
        android:padding="10dp"
        android:text="@string/str_4"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_toLeftOf="@+id/btn2"
        android:layout_below="@+id/btn4"
        android:padding="10dp"
        android:text="@string/str_1"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn0"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btn2"
        android:layout_toRightOf="@+id/btn1"
        android:padding="10dp"
        android:text="@string/str_0"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn5"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btn8"
        android:layout_toLeftOf="@+id/btn6"
        android:padding="10dp"
        android:text="@string/str_5"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@+id/btn5"
        android:layout_toLeftOf="@+id/btn3"
        android:padding="10dp"
        android:text="@string/str_2"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/btn9"
        android:padding="10dp"
        android:text="@string/str_6"
        android:textSize="30sp"/>

    <Button
        android:id="@+id/btnPlus"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_toStartOf="@+id/btn0"
        android:padding="10dp"
        android:text="@string/str_plus"
        android:textSize="30sp"
        android:layout_below="@+id/btnMinus"
 />

    <Button
        android:id="@+id/btn3"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/btn6"
        android:padding="10dp"
        android:text="@string/str_3"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btnDec"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:padding="10dp"
        android:text="@string/str_dec"
        android:textSize="30sp"
        android:layout_toRightOf="@+id/btn0"
        android:layout_below="@+id/btn3"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btnEql"
        android:layout_width="70dp"
        android:layout_height="55dp"
 android:layout_toLeftOf="@+id/btnPlus"
        android:layout_below="@+id/btnMinus"
        android:padding="10dp"
        android:text="@string/str_eq"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btn_sin"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/eTInput"
        android:padding="10dp"
        android:text="sin"
        android:textSize="30sp"/>
    <Button
        android:id="@+id/btn_cos"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_below="@+id/btn_sin"
        android:text="cos"
        />
    <Button
        android:id="@+id/btn_tan"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_toRightOf="@+id/btn_sin"
        android:layout_below="@+id/eTInput"
        android:text="tan" />
    <Button
        android:id="@+id/btn_ln"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_below="@id/btn_tan"
        android:layout_toRightOf="@id/btn_cos"
        android:text="ln" />
    <Button
        android:id="@+id/btn_log"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_below="@id/btn_cos"
        android:textSize="25sp"
        android:text="log" />
    <Button
        android:id="@+id/btn_fac"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_toRightOf="@id/btn_log"
        android:layout_below="@id/btn_ln"
        android:text="!" />


    <Button
        android:id="@+id/btn_pow"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_toRightOf="@id/btn_tan"
        android:layout_below="@id/eTInput"
        android:text="^" />
    <Button
        android:id="@+id/btn_e"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_toRightOf="@id/btn_ln"
        android:layout_below="@id/btn_pow"
        android:text="e" />
    <Button
        android:id="@+id/btn_pi"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_below="@id/btn_e"
        android:layout_toRightOf="@id/btn_fac"
        android:text="pi" />
    <Button
        android:id="@+id/btn_open_paran"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_below="@id/btn_log"
        android:text="(" />
    <Button
        android:id="@+id/btn_close_paran"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:textSize="25sp"
        android:layout_below="@id/btn_pi"
        android:layout_toRightOf="@id/btn_fac"
        android:text=")" />


</RelativeLayout>
